{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "indent":["error",4],
        "no-unused-vars":"warn",
        "no-console":"off",
        "no-trailing-spaces":"off",
        "arrow-body-style": "off"
    }
}
